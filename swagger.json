    "/devices/search": {
      "post": {
        "tags": [
          "Devices"
        ],
        "summary": "Search Devices By Owner, Device, Imei or Vin",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "token": {
                    "type": "string"
                  },
                  "field": {
                    "enum": [
                      "imei",
                      "device_name",
                      "owner_name",
                      "vin",
                    ]
                  },
                  "keyword": {
                    "type": "string"
                  }
                },
                "required": [
                  "token",
                  "field",
                  "keyword",
                ]
              }
            }
          }
        },
        "responses": {
