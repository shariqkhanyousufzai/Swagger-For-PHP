{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Slack Incoming Webhook"
  },
  "externalDocs": {
    "url": "https://api.slack.com/incoming-webhooks"
  },
  "servers": [
    {
      "url": "http://localhost/voltswitch/rest/v1"
    }
  ],
  "paths": {
    "/auth/token": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "token": {
                    "type": "string"
                  }
                },
                "required": [
                  "name",
                  "email"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/devices/getAll": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "token": {
                    "type": "string"
                  }
                },
                "required": [
                  "token",
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}